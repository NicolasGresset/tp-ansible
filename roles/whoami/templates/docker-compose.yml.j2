services:
  whoami:
    # A container that exposes an API to show its IP address
    image: traefik/whoami
    container_name: whoami{{ group_id }}
    labels:
      - "traefik.http.routers.whoami.rule=Host(`whoami{{ group_id }}.localhost`)"
    networks:
      - traefik

networks:
  traefik:
    external:
      name: traefik_proxy